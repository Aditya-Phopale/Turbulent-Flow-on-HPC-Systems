<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="20000" />
    <simulation finalTime="50.0" >
        <type>turbulenceKE</type>
        <scenario>channel</scenario>
    </simulation>
    <turbulenceParameters kappa = "0.41">
        <boundarylayer delta = "2"/>
        <constant1 cmu = "0.09"/>
        <constant2 ce = "0.07"/>
        <constant3 c1 = "0.126"/>
        <constant4 c2 = "1.92"/>
        <constant5 I = "0.2"/>
    </turbulenceParameters>
    <backwardFacingStep xRatio="0.25" yRatio="0.5" />
    <timestep dt="1e-4" tau="0.5" />
    <solver gamma="0.5" />
    <geometry
      dim="2"
      lengthX="12.0" lengthY="2.0" lengthZ="1.0"
      sizeX="120" sizeY="60" sizeZ="10"
      stretchX="false" stretchY="true" stretchZ="true"
    >
      <mesh>uniform</mesh>
      <!--mesh>stretched</mesh-->
    </geometry>
    <environment gx="0" gy="0" gz="0" />
    <walls>
        <left>
            <vector x="1.0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="0.0" y="0." z="0." />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="1">ChannelBackwardFacingStep2D</vtk>
    <stdOut interval="0.000001" />
    <parallel numProcessorsX="1" numProcessorsY="1" numProcessorsZ="1" />
</configuration>
